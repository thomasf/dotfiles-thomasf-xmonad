                    , Run MultiCpu ["-t", "<total>%", "-L","3","-H","80","--normal","#586e75","--high","#dc322f", "-c", "0", "-p", "2"] 10
                    , Run Memory ["-t","<fc=#586e75><available></fc>M"] 30
